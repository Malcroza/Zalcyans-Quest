{
	"format_version": "1.8.0",
	"animations": {
		"animation.skreepog_pilot.look_at_target.default": {
			"loop": true,
			"bones": {
				"hat": {
					"rotation": ["query.target_x_rotation", "query.target_y_rotation", 0]
				},
				"head": {
					"rotation": ["query.target_x_rotation", "query.target_y_rotation", 0]
				}
			}
		},
		"animation.skreepog_pilot.look_at_target.gliding": {
			"loop": true,
			"bones": {
				"hat": {
					"rotation": [-45, "query.target_y_rotation", 0]
				},
				"head": {
					"rotation": [-45, "query.target_y_rotation", 0]
				}
			}
		},
		"animation.skreepog_pilot.look_at_target.swimming": {
			"loop": true,
			"bones": {
				"hat": {
					"rotation": ["math.lerp(query.target_x_rotation, -45.0, variable.swim_amount)", "query.target_y_rotation", 0]
				},
				"head": {
					"rotation": ["math.lerp(query.target_x_rotation, -45.0, variable.swim_amount)", "query.target_y_rotation", 0]
				}
			}
		},
		"animation.skreepog_pilot.riding.arms": {
			"loop": true,
			"bones": {
				"leftarm": {
					"rotation": [-36, 0, 0]
				},
				"rightarm": {
					"rotation": [-36, 0, 0]
				}
			}
		},
		"animation.skreepog_pilot.riding.legs": {
			"loop": true,
			"override_previous_animation": true,
			"bones": {
				"leftleg": {
					"rotation": [-72, -18, 0]
				},
				"rightleg": {
					"rotation": [-72, 18, 0]
				}
			}
		},
		"animation.skreepog_pilot.bob": {
			"loop": true,
			"bones": {
				"leftarm": {
					"rotation": [0, 0, "-(math.cos(query.life_time * 103.2) * 2.865) - 2.865"]
				},
				"rightarm": {
					"rotation": [0, 0, "(math.cos(query.life_time * 103.2) * 2.865) + 2.865"]
				}
			}
		},
		"animation.skreepog_pilot.bow_and_arrow": {
			"loop": true,
			"bones": {
				"leftArm": {
					"rotation": [
						"query.target_x_rotation - 8.0 - math.sin(query.life_time * 76.8) * 2.865",
						"query.target_y_rotation + 18.65",
						"-(math.cos(query.life_time * 103.2) * 2.865) - 2.865"
					]
				},
				"rightArm": {
					"rotation": [
						"query.target_x_rotation - 83.0 + math.sin(query.life_time * 76.8) * 2.865",
						"query.target_y_rotation - 5.73",
						"(math.cos(query.life_time * 103.2) * 2.865) + 2.865"
					]
				},
				"body": {
					"rotation": [0, 0, 0]
				}
			}
		},
		"animation.skreepog_pilot.holding": {
			"loop": true,
			"bones": {
				"leftArm": {
					"rotation": ["variable.is_holding_left ? (0.5 - 18.0) : 0.0", 0, 0]
				},
				"rightArm": {
					"rotation": ["variable.is_holding_right ? (0.5 - 18.0) : 0.0", 0, 0]
				}
			}
		},
		"animation.skreepog_pilot.move": {
			"loop": true,
			"bones": {
				"leftArm": {
					"rotation": ["variable.is_holding_left ? 0 : Math.cos(query.modified_distance_moved * 38.17)* 57.3", 0, 0]
				},
				"leftLeg": {
					"rotation": ["Math.cos(query.modified_distance_moved * 38.17)* 57.3 * -1.4", 0, 0],
					"position": [0, "Math.cos(query.modified_distance_moved * 38.17)* 0.8", "-Math.sin((query.modified_distance_moved+1) * 38.17)* 1.4"]
				},
				"rightArm": {
					"rotation": ["variable.is_holding_right ? 0 : -Math.cos(query.modified_distance_moved * 38.17)* 57.3", 0, 0]
				},
				"rightLeg": {
					"rotation": ["Math.cos(query.modified_distance_moved * 38.17)* 57.3 * 1.4", 0, 0],
					"position": [0, "-Math.cos(query.modified_distance_moved * 38.17)* 0.8", "Math.sin((query.modified_distance_moved+1) * 38.17)* 1.4"]
				}
			}
		},
		"animation.skreepog.attack": {
			"loop": true,
			"bones": {
				"leftArm": {
					"rotation": [
						"-(-68.75 * math.sin(variable.attack_time * 180.0) + 22.92 * (math.sin((1.0 - (1.0 - variable.attack_time) * (1.0 - variable.attack_time)) * 180.0) * 6.3))",
						"5.73 - math.sin(variable.attack_time * 180.0) * 4.38",
						0
					]
				},
				"rightArm": {
					"rotation": [
						"-(-68.75 * math.sin(variable.attack_time * 180.0) + 22.92 * (math.sin((1.0 - (1.0 - variable.attack_time) * (1.0 - variable.attack_time)) * 180.0) * 6.3))",
						"5.73 - math.sin(variable.attack_time * 180.0) * 4.38",
						"5.73 - math.sin(variable.attack_time * 180.0) * 8.38"
					]
				}
			}
		}
	}
}