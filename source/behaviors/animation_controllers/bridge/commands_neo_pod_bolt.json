{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.bridge_neo_pod_bolt.execute_commands": {
			"states": {
				"default": {
					"transitions": [
						{
							"execute_command_id_1": "query.skin_id == 1"
						}
					]
				},
				"execute_command_id_1": {
					"transitions": [
						{
							"default": "query.skin_id != 1"
						}
					],
					"on_entry": [
						"/particle zalcyan:neo_pod_bolt_sparks ~ ~ ~",
						"@s bridge:remove_command_id_1"
					]
				}
			}
		},
		"controller.animation.bridge_neo_pod_bolt.execute_commands_summon": {
			"states": {
				"default": {
					"transitions": [
						{
							"execute_command_id_2": "query.variant == 1"
						}
					]
				},
				"execute_command_id_2": {
					"transitions": [
						{
							"default": "query.variant != 1"
						}
					],
					"on_entry": [
						"/playsound break.dripstone_block @a ~ ~ ~ 3 1",
						"/playsound blaster.neo.shoot @a ~ ~ ~ 3 1",
						"@s bridge:remove_command_id_2"
					]
				}
			}
		}
	}
}