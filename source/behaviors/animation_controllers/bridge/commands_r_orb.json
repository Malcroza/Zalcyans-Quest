{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.bridge_r_orb.execute_commands": {
			"states": {
				"default": {
					"transitions": [
						{
							"execute_command_id_1": "query.skin_id == 1"
						}
					],
					"on_entry": [
						"/playsound sound.lazar.shoot @a ~ ~ ~ 4.0",
						"/camerashake add @a[r=2] 0.2 0.2"
					]
				},
				"execute_command_id_1": {
					"transitions": [
					],
					"on_entry": [
						"/particle zalcyan:r_orb_sparks ~ ~ ~",
						"@s bridge:remove_command_id_1",
						"/playsound sound.orb.ricochet @a ~ ~ ~ 1.2"
					]
				}
			}
		}
	}
}