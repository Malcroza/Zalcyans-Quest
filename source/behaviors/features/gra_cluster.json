{
    "format_version": "1.13.0",
    
    "minecraft:scatter_feature": {
        "description": {
            "identifier": "zalcyan:gra_cluster"
        },
        
        "places_feature": "zalcyan:gra_structure",
        
        "iterations": "math.random_integer(2, 6)",
        
        "x": {
            "distribution": "gaussian",
            "extent": [-2, 2]
